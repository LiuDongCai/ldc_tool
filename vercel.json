{
    "version": 2,
    "public": false,
    "builds": [
        {
            "src": "**/*.dart",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build/web"
            }
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "build/web/index.html"
        },
        {
            "src": "/main/list",
            "dest": "assets/json/eat/eat_main.json"
        },
        {
            "src": "/drink/list",
            "dest": "assets/json/eat/eat_drink.json"
        },
        {
            "src": "/dessert/list",
            "dest": "assets/json/eat/eat_dessert.json"
        }
    ]
}